{
  "name": "@intellihr/ui-components",
  "description": "A common React components library that is used in our company",
  "version": "1.0.5-beta.10",
  "main": "./dist/",
  "types": "./es/index.d.ts",
  "files": [
    "es",
    "dist"
  ],
  "dependencies": {},
  "peerDependencies": {
    "foundation-sites": "^6.4.3"
  },
  "devDependencies": {
    "@epegzz/sass-vars-loader": "^3.2.0",
    "@intellihr/standard": "^11.0.3",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.2.0",
    "@types/jquery": "^3.3.1",
    "@types/lodash": "^4.14.104",
    "@types/react": "^16.0.40",
    "awesome-typescript-loader": "^4.0.1",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint-plugin-typescript": "^0.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "foundation-datepicker": "1.5.3",
    "foundation-sites": "^6.4.3",
    "jest": "^22.4.2",
    "jquery": "^3.3.1",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.1",
    "react": "^16.2.0",
    "react-docgen-typescript": "^1.2.5",
    "react-dom": "^16.2.0",
    "react-styleguidist": "https://github.com/intellihr/react-styleguidist.git#357d8ddf375a3669a8f6a8a2753a6212ea11f0ed",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "snazzy": "^7.1.1",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10"
  },
  "scripts": {
    "i": "yarn && npm rebuild node-sass",
    "clean": "rm -rf dist/* && rm -rf es/*",
    "compile": "rm -rf es/* && tsc",
    "test": "NODE_ENV=test && jest --no-cache",
    "lint": "standard **/*.{ts,tsx,js,jsx} --verbose | snazzy",
    "lint:fix": "standard **/*.{ts,tsx,js,jsx} --verbose --fix | snazzy",
    "build": "yarn clean && webpack",
    "build:watch": "yarn build -d --watch",
    "styleguide:build": "styleguidist build",
    "styleguide": "styleguidist server",
    "gh-pages": "git subtree push --prefix styleguide origin gh-pages",
    "codequality": "docker run --env CODECLIMATE_FORMAT=html --env SOURCE_CODE=$(pwd) --volume $(pwd):/code --volume /var/run/docker.sock:/var/run/docker.sock quay.io/intellihr/codequality /code"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellihr/ui-components.git"
  },
  "keywords": [
    "intellihr",
    "components",
    "ui-components",
    "react"
  ],
  "author": "Nick Luo <nick.luo@intellihr.com.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/intellihr/ui-components/issues"
  },
  "homepage": "https://github.com/intellihr/ui-components#readme",
  "standard": {
    "parser": "typescript-eslint-parser",
    "envs": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ],
    "ignore": [
      "node_modules/**",
      "styleguide/**"
    ]
  }
}
