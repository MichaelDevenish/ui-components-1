    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-node-externals": "1.7.2"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "clean": "rm -rf dist/* && rm -rf es/*",
    "compile": "rm -rf es/* && tsc",
    "test": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --silent --ci --coverage --colors",
    "lint": "standard \"**/*.{ts,tsx,js,jsx}\" --verbose | snazzy",
    "lint:fix": "standard \"**/*.{ts,tsx,js,jsx}\" --verbose --fix | snazzy",
    "lint:css": "stylelint \"src/**/*.{ts,tsx}\"",
    "lint:sass": "sass-lint --verbose",
    "lint:sass:fix": "sass-lint-auto-fix -c .sass-lint.yml --verbose",
    "build": "npm run clean && webpack && mkdir -p dist/sass && cp -r src/sass dist",
    "build:watch": "webpack --watch",
    "styleguide:build": "styleguidist build",
    "styleguide": "styleguidist server",
    "gh-pages": "git subtree push --prefix styleguide origin gh-pages",
    "codequality": "docker run --env CODECLIMATE_FORMAT=html --env SOURCE_CODE=$(pwd) --volume $(pwd):/code --volume /var/run/docker.sock:/var/run/docker.sock quay.io/intellihr/codequality /code"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellihr/ui-components.git"
  },
  "keywords": [
    "intellihr",
    "components",
    "ui-components",
    "react"
  ],
  "author": "Nick Luo <nick.luo@intellihr.com.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/intellihr/ui-components/issues"
  },
  "homepage": "https://github.com/intellihr/ui-components#readme",
  "standard": {
    "parser": "typescript-eslint-parser",
    "envs": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ],
    "ignore": [
      "node_modules/**",
      "styleguide/**"
    ]
  },
  "dependencies": {
    "uuid": "^3.2.1"
  }
}
